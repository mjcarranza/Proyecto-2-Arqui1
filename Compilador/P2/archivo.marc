    beq _bresenham:
    cmp r4, #0x52              @ R
    beq _bresenham:
    cmp r4, #0x53              @ S
    beq _bresenham:
    cmp r4, #0x54              @ T
    beq _bresenham:
    cmp r4, #0x5a              @ Z
    beq _bresenham:

    set r0, #2                  
    set r1, #10 
    set r2, #9                  
    set r3, #10

    cmp r4, #0x42              @ B
    beq _bresenham:

    set r0, #2                  
    set r1, #10                  
    set r2, #10                  
    set r3, #9

    cmp r4, #0x44              @ D
    beq _bresenham:

    set r0, #2                  
    set r1, #6                  
    set r2, #10                  
    set r3, #2
    
    cmp r4, #0x4b              @ K
    beq _bresenham:

    set r0, #2                  
    set r1, #10                  
    set r2, #10                  
    set r3, #2    
    
    cmp r4, #0x4e              @ N
    beq _bresenham:
    cmp r4, #0x58              @ X
    beq _bresenham:

    set r0, #6                  
    set r1, #2                  
    set r2, #10                  
    set r3, #10
 
    cmp r4, #0x56              @ V
    beq _bresenham:

    set r0, #6                  
    set r1, #6                  
    set r2, #10                  
    set r3, #10

    cmp r4, #0x59              @ Y
    beq _bresenham:

_stepIII:
    add r12, r12, r10

    set r0, #2                  
    set r1, #2                  
    set r2, #10                  
    set r3, #2

    cmp r4, #0x42              @ B
    beq _bresenham:
    cmp r4, #0x43              @ C
    beq _bresenham:
    cmp r4, #0x45              @ E
    beq _bresenham:
    cmp r4, #0x47              @ G
    beq _bresenham:
    cmp r4, #0x49              @ I
    beq _bresenham:
    cmp r4, #0x4c              @ L
    beq _bresenham:
    cmp r4, #0x4f              @ O
    beq _bresenham:
    cmp r4, #0x51              @ Q
    beq _bresenham:
    cmp r4, #0x53              @ S
