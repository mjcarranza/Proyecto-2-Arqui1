_start:
    set r0, r13                     @ r0: PU
    set r1, #1                      @ r1: 1
    not
    not
    not
    str r1, [r0]        
    ldr r2, [r0]
    not
    not
    not
    cmp r1, r2
    beq _success:
    not
    not
    bal _error:
    not
    not

_success:
    set r3, #1
    bal _exit:
    not
    not

_error:
    set r3, #2

_exit:
    end
